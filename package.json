{
  "name": "portal-template",
  "private": true,
  "version": "0.0.1",
  "description": "Redocly portal template repository",
  "license": "MIT",
  "scripts": {
    "start": "node ./scripts/build.js && redocly-portal develop -p 3000",
    "build": "redocly-portal build",
    "clean": "redocly-portal clean",
    "deploy": "npm run build && deploy-to-gh-pages --update web_deploy",
    "deploy-branch": "node ./scripts/deploy-branch.js",
    "test": "swagger-cli validate --no-schema web_deploy/swagger.yaml",
    "swagger": "swagger-repo",
    "sync-generated-apm-specs": "node ./scripts/sync-generated-apm-specs.js",
    "sync-generated-apm-specs-legacy-aws-env": "node ./scripts/sync-generated-apm-specs-legacy-aws-env.js"
  },
  "dependencies": {
    "@redocly/developer-portal": "1.0.0-beta.120",
    "bower": "^1.7.7",
    "connect": "^3.4.1",
    "cors": "^2.7.1",
    "deploy-to-gh-pages": "^1.1.0",
    "gulp": "^4.0.2",
    "gulp-connect": "^5.7.0",
    "gulp-exec": "^4.0.0",
    "gulp-file-include": "^2.3.0",
    "gulp-util": "^3.0.8",
    "portfinder": "^1.0.26",
    "request": "2.88.0",
    "shelljs": "^0.8.4",
    "swagger-cli": "2.1.0",
    "swagger-repo": "^1.5.1",
    "swagger-ui": "^2.2.10",
    "yamljs": "0.3.0"
  }
}
